         LTR   R15,R15
         BNZ   SUB01025
SUB01055 EQU   *
         CALL  ISPLINK,(LMMFIND,P_DATAID,P_MEM),VL
SUB01060 EQU   *
         CALL  ISPLINK,(LMGET,P_DATAID,INVAR,DATALOC,LENVAR,L256),VL
         LTR   R15,R15
         BNZ   SUB01025
         AP    TMAX(03),=P'1'
         CP    TMAX(03),PMAX(03)
         BH    SUB01025
         MVC   QWKF_T(256),P_DATA
         MVC   QWKF_I(078),P_PV2
