SUB01085 EQU   *
         LA    R5,P_MEM
         AR    R5,R4
         CLI   0(R5),C' '
         BNE   SUB01090
         BCT   R4,SUB01085
         ABEND 99,REASON=8
SUB01090 EQU   *
         MVC   1(2,R5),=X'5D7D'
         MVC   1(10,R2),P_MEM
         MVC   PANEL(08),=CL8'QWIKSCN3'
         CALL  ISPLINK,(CONTROL,DISPLAY,SVE),VL
         CALL  ISPLINK,(BROWSE,DSN,VOL,PWD,PANEL),VL
         CALL  ISPLINK,(CONTROL,DISPLAY,RST),VL
         L     R15,TBRC
         C     R15,=F'0'
         BE    SUB01070
         MVC   P_MEM(10),=CL10' '
         CALL  ISPLINK,(TBDISPL,QWIKTBL),VL
         ST    R15,TBRC
         B     SUB01075
